{
  "name": "OneShortArena_Roblox",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Assets": {
        "$path": "src/ReplicatedStorage/Assets"
      },
      "Configs": {
        "$path": "src/ReplicatedStorage/Configs"
      },
      "Shared": {
        "$path": "src/ReplicatedStorage/Shared"
      },
      "UI": {
        "$path": "src/ReplicatedStorage/UI"
      },
      "SystemsShared": {
        "$path": "src/ReplicatedStorage/SystemsShared"
      },

      "Utils": {
        "$path": "src/ReplicatedStorage/Utils"
      },

      "Packages": {
        "$path": "Packages"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Services": {
        "$path": "src/ServerScriptService/Services"
      },
      "Utils": {
        "$path": "src/ServerScriptService/Utils"
      },
      "Init": {
        "$path": "src/ServerScriptService/Init.server.luau"
      }
    },

    "ServerStorage": {
      "$className": "ServerStorage",
      "Configs": {
        "$path": "src/ServerStorage/Configs"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",

        "Core": {
          "$path": "src/StarterPlayer/StarterPlayerScripts/Core"
        },
        "Dev": {
          "$path": "src/StarterPlayer/StarterPlayerScripts/Dev"
        },
        "Gameplay": {
          "$path": "src/StarterPlayer/StarterPlayerScripts/Gameplay"
        },
        "Inputs": {
          "$path": "src/StarterPlayer/StarterPlayerScripts/Inputs"
        },
        "UI":{
          "$path": "src/StarterPlayer/StarterPlayerScripts/UI"
        },
        "Init": {
          "$path": "src/StarterPlayer/StarterPlayerScripts/Init.client.luau"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [0.388, 0.372, 0.388],
          "Locked": true,
          "Position": [0, -10, 0],
          "Size": [512, 20, 512]
        }
      }
    },

    "Lighting": {
      "$properties": {
        "Ambient": [0, 0, 0],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}